<h2 class="center">User Table for <%= user.name %>
</h2>
<table class="table">
    <tbody>
        <tr>
            <td>Name</td>
            <td>
                <%= user.name %>
            </td>
        <tr>
            <td>Email</td>
            <td>
                <%= user.email %>
            </td>
        </tr>
        <tr>
            <td>ZipCode</td>
            <td>
                <%= user.zipCode %>
            </td>
        </tr>
    </tbody>
</table>